{
    "name": "mahmud/laravel-utilities",
    "description": "Bunch of Laravel utilities those are not included to the core of the framework",
    "license": "MIT",
    "authors": [
        {
            "name": "MD. Mahmud Ur Rahman",
            "email": "mahmudkuet11@gmail.com",
            "homepage": "https://mahmud.live"
        }
    ],
    "homepage": "https://github.com/mahmudkuet11/laravel-utilities",
    "keywords": ["Laravel", "Laravel Utilities"],
    "require": {
        "illuminate/support": "~7",
        "illuminate/contracts": "~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~5",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mahmud\\LaravelUtilities\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mahmud\\LaravelUtilities\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mahmud\\LaravelUtilities\\LaravelUtilitiesServiceProvider"
            ]
        }
    }
}
